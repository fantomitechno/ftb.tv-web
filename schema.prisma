datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Command {
  commandName String  @id
  message     String?
  reply       Boolean @default(true)
  isMod       Boolean @default(false)
}

model Token {
  accessToken  String   @unique
  expiresIn    Int
  createdAt    DateTime @default(now())
  refreshToken String
}

model Template {
  title   String @id
}
